namespace my.products;

entity Products {
  @title : 'ID' key ID : Integer;
  @title : 'Description' Description : String;
  @title : 'Price' Price : Integer;
  @title : 'Weight' Weight  : Integer;
  @title : 'Color' Color  : String;
  @title : 'Height' Height  : Integer;
  @title : 'Width' Width  : Integer;
  @title : 'Depth' Depth  : Integer;

}